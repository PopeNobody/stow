#!/usr/bin/perl
#! -*- perl -*- -p
eval 'exec perl -x -wS $0 ${1+"$@"}'
  if 0;

use strict;
use warnings;
use autodie qw(:all);
BEGIN {
  use FindBin qw($Bin);
  use lib "$Bin/../lib/perl";
  $|++;
  $\="\n"; $,=" "; $"=" ";
};
use Data::Dumper;

open(STDIN,"dpkg -l|");
my %seen;
while(<STDIN>){
  print STDERR "$_";
  $_=substr($_,0,5) if length > 5;
  $seen{$_}=1;
};
print sort keys %seen;
