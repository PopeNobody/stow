#!/usr/bin/perl
use Data::Dump qw(pp dd);
use strict;
use warnings;
use Carp;


my (@select,@order);
$\="\n";
$,=" ";
my (@o);

for my $i (0 .. @ARGV-1){
  for($ARGV[$i]){
    next unless /^[\d,]+$/;
    push(@o,split /,/);
    $_=undef;
  };
}
@ARGV = grep { defined } @ARGV;
die "bad args: @ARGV\n" if grep { !-e }  @ARGV;
if(@o){
  while(<STDIN>){
    next unless @_=split;
    my (@t)=@_[@o];
    @_=@t;
    print @_;
  };
} else {
  while(<STDIN>){
    next unless /\S/;
    print split;
  }
}
exit(0);
