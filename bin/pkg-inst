#!/usr/bin/perl
#! -*- perl -*- -p
eval 'exec perl -x -wS $0 ${1+"$@"}'
  if 0;

use strict;
use warnings;
BEGIN {
  use FindBin qw($Bin);
  use lib "$Bin/../lib/perl";
  $|++;
  $\="\n"; $,=" "; $"=" ";
  use AutoLess;
};
use autodie qw(:all);
use Data::Dumper;

open(STDIN,"dpkg -l|");
my %seen;
while(<STDIN>){
	chomp;
	if(s{^ii\s+(\S+).*}{$1}){
    s{:.*}{};
    print;
  };
};
